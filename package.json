{
  "name": "@stellaraf/utilities",
  "version": "1.0.2",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Matt Love <matt@stellar.tech>",
  "license": "BSD-3-Clause",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist/",
    "esm/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sindresorhus/is": "^4.6.0",
    "dayjs": "^1.11.5",
    "merge-anything": "^4.0.1"
  },
  "devDependencies": {
    "@stellaraf/eslint-config": "^1.1.2",
    "@stellaraf/jest": "^1.0.0",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.18",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.1",
    "jest": "^28.1.3",
    "jest-esbuild": "^0.2.9",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "lint": "eslint --quiet src/**/*",
    "format": "prettier --loglevel warn -c .",
    "typecheck": "tsc --build",
    "build": "ts-node-esm build.ts && tsc --emitDeclarationOnly",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@stellaraf"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "semi": true
  }
}
